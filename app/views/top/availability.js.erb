var markerArray = new Array();

<% @jalan.each { |v| val = v[1] %>

	var content = "<a href='<%= val['HotelDetailURL'] %>' target='_blank'><%= val['HotelName'] %></a><hr/>";
	content += "最安プラン：<%= val['RoomName'] %><br/>";
	content += "料金：<%= val['Rate'] %><br/>";
	content += "空き室数：<%= val['Stock'] %><br/>";

	markerArray.push({
		position: new google.maps.LatLng("<%= val['X'] %>","<%= val['Y'] %>"),
		title: "<%= val['HotelName'] %>",
		content: content,
		rate: "<%= val['Rate'] %>"
	});
<% } %>

setMarkerData(markerArray);

// コンビニ検索
placeSearch();

